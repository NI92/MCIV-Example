source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '13.0'

def shared_pods
    	#pod 'KeychainAccess'
    	pod 'KeychainSwift'
	pod 'ReachabilitySwift'
    	pod 'PromiseKit'
	pod 'PromiseKit/Alamofire'
    	pod 'Alamofire'
    	pod 'Kingfisher'
    	pod 'SVProgressHUD'
	## Image caching
    	#pod 'AlamofireImage'
	## Network Activity Indicator Manager
    	#pod 'AlamofireNetworkActivityIndicator'
end

target 'MCIV Example' do
	use_frameworks!
	shared_pods
end
